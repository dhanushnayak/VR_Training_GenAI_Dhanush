   "source": [
    "### 2.4 Multiple Inheritance and Method Resolution Order (MRO)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Multiple inheritance example\n",
    "class Flyable:\n",
    "    \"\"\"Mixin class for flying ability.\"\"\"\n",
    "    \n",
    "    def fly(self):\n",
    "        return f\"{self.name} is flying high!\"\n",
    "    \n",
    "    def land(self):\n",
    "        return f\"{self.name} has landed safely.\"\n",
    "\n",
    "class Swimmable:\n",
    "    \"\"\"Mixin class for swimming ability.\"\"\"\n",
    "    \n",
    "    def swim(self):\n",
    "        return f\"{self.name} is swimming gracefully!\"\n",
    "    \n",
    "    def dive(self):\n",
    "        return f\"{self.name} dives underwater.\"\n",
    "\n",
    "class Bird(Animal, Flyable):\n",
    "    \"\"\"Bird class with flying ability.\"\"\"\n",
    "    \n",
    "    def __init__(self, name, wingspan):\n",
    "        super().__init__(name, \"Avian\")\n",
    "        self.wingspan = wingspan\n",
    "    \n",
    "    def make_sound(self):\n",
    "        return f\"{self.name} chirps: Tweet! Tweet!\"\n",
    "\n",
    "class Duck(Bird, Swimmable):\n",
    "    \"\"\"Duck class that can both fly and swim.\"\"\"\n",
    "    \n",
    "    def __init__(self, name):\n",
    "        super().__init__(name, \"Medium\")\n",
    "    \n",
    "    def make_sound(self):\n",
    "        return f\"{self.name} quacks: Quack! Quack!\"\n",
    "\n",
    "class Penguin(Bird, Swimmable):\n",
    "    \"\"\"Penguin class that can swim but not fly.\"\"\"\n",
    "    \n",
    "    def __init__(self, name):\n",
    "        super().__init__(name, \"Small\")\n",
    "    \n",
    "    def fly(self):  # Override to prevent flying\n",
    "        return f\"{self.name} can't fly, but waddles instead!\"\n",
    "    \n",
    "    def make_sound(self):\n",
    "        return f\"{self.name} makes penguin sounds!\"\n",
    "\n",
    "# Using multiple inheritance\n",
    "duck = Duck(\"Donald\")\n",
    "penguin = Penguin(\"Pingu\")\n",
    "\n",
    "print(\"=== Duck (can fly and swim) ===\")\n",
    "print(duck.make_sound())\n",
    "print(duck.fly())\n",
    "print(duck.swim())\n",
    "print(duck.dive())\n",
    "print()\n",
    "\n",
    "print(\"=== Penguin (can swim but not fly) ===\")\n",
    "print(penguin.make_sound())\n",
    "print(penguin.fly())  # Overridden method\n",
    "print(penguin.swim())\n",
    "print(penguin.dive())\n",
    "print()\n",
    "\n",
    "# Method Resolution Order (MRO)\n",
    "print(\"Method Resolution Order:\")\n",
    "print(f\"Duck MRO: {Duck.__mro__}\")\n",
    "print(f\"Penguin MRO: {Penguin.__mro__}\")\n",
    "print()\n",
    "\n",
    "# Check multiple inheritance\n",
    "print(f\"Is duck a Bird? {isinstance(duck, Bird)}\")\n",
    "print(f\"Is duck Flyable? {isinstance(duck, Flyable)}\")\n",
    "print(f\"Is duck Swimmable? {isinstance(duck, Swimmable)}\")"
   ]
