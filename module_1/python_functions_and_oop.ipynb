  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 2.3 Inheritance - Single and Multiple Inheritance"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Base class (Parent class)\n",
    "class Animal:\n",
    "    \"\"\"Base Animal class.\"\"\"\n",
    "    \n",
    "    def __init__(self, name, species):\n",
    "        self.name = name\n",
    "        self.species = species\n",
    "        self.is_alive = True\n",
    "    \n",
    "    def eat(self):\n",
    "        return f\"{self.name} is eating.\"\n",
    "    \n",
    "    def sleep(self):\n",
    "        return f\"{self.name} is sleeping.\"\n",
    "    \n",
    "    def make_sound(self):\n",
    "        return f\"{self.name} makes a sound.\"\n",
    "    \n",
    "    def __str__(self):\n",
    "        return f\"{self.name} ({self.species})\"\n",
    "\n",
    "# Derived class (Child class)\n",
    "class Dog(Animal):\n",
    "    \"\"\"Dog class inheriting from Animal.\"\"\"\n",
    "    \n",
    "    def __init__(self, name, breed):\n",
    "        # Call parent constructor\n",
    "        super().__init__(name, \"Canine\")\n",
    "        self.breed = breed\n",
    "        self.tricks = []\n",
    "    \n",
    "    def make_sound(self):  # Method overriding\n",
    "        return f\"{self.name} barks: Woof! Woof!\"\n",
    "    \n",
    "    def fetch(self):\n",
    "        return f\"{self.name} fetches the ball!\"\n",
    "    \n",
    "    def learn_trick(self, trick):\n",
    "        self.tricks.append(trick)\n",
    "        return f\"{self.name} learned {trick}!\"\n",
    "    \n",
    "    def perform_tricks(self):\n",
    "        if not self.tricks:\n",
    "            return f\"{self.name} doesn't know any tricks yet.\"\n",
    "        return f\"{self.name} performs: {', '.join(self.tricks)}\"\n",
    "\n",
    "class Cat(Animal):\n",
    "    \"\"\"Cat class inheriting from Animal.\"\"\"\n",
    "    \n",
    "    def __init__(self, name, color):\n",
    "        super().__init__(name, \"Feline\")\n",
    "        self.color = color\n",
    "        self.lives = 9\n",
    "    \n",
    "    def make_sound(self):  # Method overriding\n",
    "        return f\"{self.name} meows: Meow! Meow!\"\n",
    "    \n",
    "    def climb(self):\n",
    "        return f\"{self.name} climbs up the tree!\"\n",
    "    \n",
    "    def use_life(self):\n",
    "        if self.lives > 0:\n",
    "            self.lives -= 1\n",
    "            return f\"{self.name} used a life. {self.lives} lives remaining.\"\n",
    "        return f\"{self.name} has no more lives!\"\n",
    "\n",
    "# Using inheritance\n",
    "dog = Dog(\"Buddy\", \"Golden Retriever\")\n",
    "cat = Cat(\"Whiskers\", \"Orange\")\n",
    "\n",
    "print(\"=== Dog ====\")\n",
    "print(dog)  # Uses inherited __str__ method\n",
    "print(dog.eat())  # Inherited method\n",
    "print(dog.make_sound())  # Overridden method\n",
    "print(dog.fetch())  # Dog-specific method\n",
    "print(dog.learn_trick(\"sit\"))\n",
    "print(dog.learn_trick(\"roll over\"))\n",
    "print(dog.perform_tricks())\n",
    "print()\n",
    "\n",
    "print(\"=== Cat ====\")\n",
    "print(cat)\n",
    "print(cat.sleep())  # Inherited method\n",
    "print(cat.make_sound())  # Overridden method\n",
    "print(cat.climb())  # Cat-specific method\n",
    "print(cat.use_life())\n",
    "print(cat.use_life())\n",
    "print()\n",
    "\n",
    "# Check inheritance\n",
    "print(f\"Is dog an Animal? {isinstance(dog, Animal)}\")\n",
    "print(f\"Is dog a Dog? {isinstance(dog, Dog)}\")\n",
    "print(f\"Is dog a Cat? {isinstance(dog, Cat)}\")"
   ]
  },
